{
  "name": "minecraft-bot",
  "version": "1.0.0",
  "description": "Minecraft Discord bot for noobs",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.ts'",
    "register:commands": "ts-node src/commands/register.ts",
    "postinstall": "npx simple-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigomata/minecraft-bot.git"
  },
  "keywords": [
    "minecraft"
  ],
  "author": "Rodrigo Mata",
  "license": "MIT",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/rodrigomata/minecraft-bot/issues"
  },
  "homepage": "https://github.com/rodrigomata/minecraft-bot#readme",
  "devDependencies": {
    "@middy/core": "^4.3.0",
    "@middy/error-logger": "^4.3.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.2.1",
    "npm": "^9.6.4",
    "prettier": "^2.8.7",
    "serverless": "^3.30.1",
    "serverless-bundle": "^6.0.0",
    "simple-git-hooks": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.1",
    "@discordjs/rest": "^1.7.0",
    "algoliasearch": "^4.17.0",
    "tweetnacl": "^1.0.3"
  }
}
